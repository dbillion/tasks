  "scripts": {
     "start": "vite", 
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:component": "cypress run --component",
    "report:merge": "mochawesome-merge \"cypress/reports/*.json\" > cypress/reports/output.json",
    "report:generate": "marge cypress/reports/output.json --reportDir cypress/reports --inline true --charts true --pdf true",
    "test:report": "npm run clean:reports && npm run test:component && npm run report:merge && npm run report:generate",
    "clean:reports": "rm -rf cypress/reports/* || true",
    "prereport": "npm run clean:reports",
    "cy:run": "cypress run --component",
    "merge:reports": "mochawesome-merge \"cypress/reports/separate/*.json\" > cypress/reports/output.json",
    "create:html": "marge cypress/reports/output.json --reportDir cypress/reports/html --inline true --charts true --pdf true",
    "test:parallel": "jest --maxWorkers=50%",
    "test:parallel-report": "npm run prereport && npm run test:parallel && npm run merge:reports && npm run create:html",
    "test:coverage": "jest --coverage",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "cy:parallel": "cypress run --record --key your-record-key --parallel --component",
    "test:ci": "start-server-and-test dev http://localhost:5173 cy:parallel"
  },
